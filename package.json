{
  "name": "markdown-it-dollarmath",
  "version": "0.4.2",
  "description": "A markdown-it plugin for $-delimited math.",
  "author": "Chris Sewell",
  "homepage": "https://github.com/executablebooks/markdown-it-dollarmath",
  "license": "MIT",
  "keywords": [
    "markdown",
    "markdown-it",
    "markdown-it-plugin",
    "latex",
    "math"
  ],
  "type": "module",
  "files": [
    "src",
    "dist"
  ],
  "exports": "./dist/index.js",
  "types": "dist/index.d.ts",
  "unpkg": "dist/index.umd.min.js",
  "scripts": {
    "clean": "rimraf dist",
    "format": "prettier --write src/**/*.ts tests/**/*.ts",
    "lint": "eslint -c .eslintrc.yml --max-warnings 1 src/**/*.ts tests/**/*.ts",
    "lint:fix": "eslint -c .eslintrc.yml --fix src/**/*.ts tests/**/*.ts",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "prebuild": "rm -rf dist",
    "build:bundles": "rollup -c",
    "build:esm": "tsc --module node16 --outDir dist",
    "build": "npm-run-all -l clean -p build:esm build:bundles",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=6"
  },
  "peerDependencies": {
    "markdown-it": "^12 || ^13"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/katex": "^0.16.3",
    "@types/markdown-it": "^13.0.1",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "@vitest/coverage-v8": "^0.34.4",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "katex": "^0.16.8",
    "markdown-it": "^12.3.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "rollup": "^2.67.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "typescript": "^5.2.2",
    "vitest": "^0.34.4"
  }
}
